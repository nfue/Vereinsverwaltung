@{
}

<h1>Gruppenteilnehmer</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Username</th>
            <th scope="col">Interessengruppe</th>
        </tr>
    </thead>

    <tbody>       
            @foreach(var gruppenteilnehmer in Model)
            {
                <tr>
                    <td>@gruppenteilnehmer.Username</td>
                    <td>@gruppenteilnehmer.Interessengruppe</td>
                </tr>
            }      
    </tbody>
</table>